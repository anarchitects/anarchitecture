{
  "name": "anarchitects-storybook",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/app-specific/anarchitects/storybook/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../../../coverage/libs/app-specific/anarchitects/storybook"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/app-specific/anarchitects/storybook/.storybook",
        "browserTarget": "anarchitects-storybook:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/anarchitects-storybook",
        "configDir": "libs/app-specific/anarchitects/storybook/.storybook",
        "browserTarget": "anarchitects-storybook:build-storybook",
        "compodoc": false,
        "assets": [
          {
            "input": "libs/app-specific/anarchitects/ui/hero/src/assets",
            "glob": "**/*",
            "output": "assets"
          }
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c libs/app-specific/anarchitects/storybook/.storybook --url=http://localhost:4400"
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "dependsOn": ["build-storybook"],
      "options": {
        "buildTarget": "anarchitects-storybook:build-storybook",
        "staticFilePath": "dist/storybook/anarchitects-storybook",
        "spa": true
      },
      "configurations": {
        "ci": {
          "buildTarget": "anarchitects-storybook:build-storybook:ci"
        }
      }
    }
  }
}
